    <PackOnBuild>true</PackOnBuild>
    <PackageVersion>0.1</PackageVersion>
    <Authors>Jose Jimenez, Quinn (Qin) Liu , Don Pham , John Gaines </Authors>
    <PackageProjectUrl>https://github.com/masters3d/CSharp-ConsoleView</PackageProjectUrl>
    <PackageTags>console, text</PackageTags>
    <Description>Simple C# Console View Helper</Description>
    <SynchReleaseVersion>false</SynchReleaseVersion>
